<%= form_for([@product, @product.bookings.new]) do |f| %>
<%= f.text_field :from_date %>
<%= f.text_field :to_date %>

<br/>
<%= f.submit "Book Now", { :class => 'btn btn-normal btn-block', 'data-toggle' => 'modal', 'data-target' => '#exampleModalCenter'} %>
<% end %>

<script>
  $(function(){
    $('#booking_from_date').datepicker({
      dateFormat: 'dd-mm-yy'
    });
    $('#booking_to_date').datepicker({
      dateFormat: 'dd-mm-yy'
    });
  });
</script>
